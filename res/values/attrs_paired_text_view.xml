<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="PairedTextView">
        <attr name="labelText" format="string" />
        <attr name="contentText" format="string" />
        <attr name="contentPosition" format="enum">
                <enum name="below" value="0" />
                <enum name="above" value="1" />
                <enum name="rightOf" value="2" />
                <enum name="leftOf" value="3" />
        </attr>
        <attr name="textSize" format="dimension" />
    </declare-styleable>
</resources>